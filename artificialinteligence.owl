<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:ex="http://example.org#">

  <!-- Superclass for Operation -->
  <rdf:Description rdf:about="http://example.org#Operation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Operation</rdfs:label>
  </rdf:Description>

  <!-- Subclass Addition -->
  <rdf:Description rdf:about="http://example.org#Addition">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Addition</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Subtraction -->
  <rdf:Description rdf:about="http://example.org#Subtraction">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Subtraction</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Multiplication -->
  <rdf:Description rdf:about="http://example.org#Multiplication">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Multiplication</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Division -->
  <rdf:Description rdf:about="http://example.org#Division">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Division</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Exponentiation -->
  <rdf:Description rdf:about="http://example.org#Exponentiation">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Exponentiation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Square Root -->
  <rdf:Description rdf:about="http://example.org#SquareRoot">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Square Root</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Modulus -->
  <rdf:Description rdf:about="http://example.org#Modulus">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Modulus</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Subclass Percentage -->
  <rdf:Description rdf:about="http://example.org#Percentage">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Percentage</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://example.org#Operation"/>
  </rdf:Description>

  <!-- Property for number values -->
  <rdf:Description rdf:about="http://example.org#hasNumber1">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://example.org#Operation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

  <rdf:Description rdf:about="http://example.org#hasNumber2">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://example.org#Operation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

  <!-- Property for result -->
  <rdf:Description rdf:about="http://example.org#hasResult">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://example.org#Operation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  </rdf:Description>

</rdf:RDF>